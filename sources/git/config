[color]
	ui          = auto
	status      = auto
	diff        = auto
	branch      = auto
	interactive = auto
	grep        = auto
[core]
	editor = vim
	pager  = lv -c
[alias]
	l  = log --pretty=format:"%C(yellow)%h%Cblue%d%Creset %s %C(black bold)%an, %ar%Creset" --graph --all
	s  = status
	c  = commit
	cm = checkout master
[include]
	path = ~/.config/git/config.local
[merge]
	ff = false
